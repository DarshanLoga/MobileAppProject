  <target name="compile.module.cktest-app.production" depends="register.custom.compilers" description="Compile module cktest-app; production classes">
    <mkdir dir="${cktest-app.output.dir}"/>
    <javac2 destdir="${cktest-app.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.cktest-app}/javac">
      <compilerarg line="${compiler.args.cktest-app}"/>
      <bootclasspath refid="cktest-app.module.bootclasspath"/>
      <classpath refid="cktest-app.module.production.classpath"/>
      <src refid="cktest-app.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.cktest-app"/>
    </javac2>
    
    <copy todir="${cktest-app.output.dir}">
      <fileset dir="${module.cktest-app.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.cktest-app.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.cktest-app.tests" depends="register.custom.compilers,compile.module.cktest-app.production" description="compile module cktest-app; test classes" unless="skip.tests">
    <mkdir dir="${cktest-app.testoutput.dir}"/>
    <javac2 destdir="${cktest-app.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.cktest-app}/javac">
      <compilerarg line="${compiler.args.cktest-app}"/>
      <bootclasspath refid="cktest-app.module.bootclasspath"/>
      <classpath refid="cktest-app.module.classpath"/>
      <src refid="cktest-app.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.cktest-app"/>
    </javac2>
    
    <copy todir="${cktest-app.testoutput.dir}">
      <fileset dir="${module.cktest-app.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
